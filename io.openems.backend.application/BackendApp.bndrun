	Java-WebSocket;version='[1.5.2,1.5.3)',\
	com.google.gson;version='[2.8.9,2.8.10)',\
	com.google.guava;version='[31.0.1,31.0.2)',\
	com.google.guava.failureaccess;version='[1.0.1,1.0.2)',\
